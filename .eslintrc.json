{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": ["error", "ignorePackages"],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "operator-linebreak": "off",
        "object-curly-newline": "off",
        "indent": ["error", 4]
    }
}
